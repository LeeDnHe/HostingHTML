{
  "headers": [
    {
      "source": "/(.*)\\.br$",
      "headers": [
        { "key": "Content-Encoding", "value": "br" },
        { "key": "Content-Type", "value": "application/octet-stream" }
      ]
    }
  ],
  "overrides": {
    "StreamingAssets/aa/WebGL/*.bundle": {
      "contentType": "application/octet-stream",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    }
  }
}